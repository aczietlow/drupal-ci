{
    "name": "zietlow/drupal-ci",
    "type": "project",
    "description": "Project to prove Drupal can use a good CI workflow.",
    "minimum-stability": "dev",
    "type": "project",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.drupal-composer.org"
        }
    ],
    "authors": [
        {
            "name": "chris.zietlow",
            "email": "aczietlow@gmail.com"
        }
    ],
    "require": {
        "composer/installers": "^1.0.20",
        "cweagans/composer-patches": "~1.0",
        "drupal/acquia_connector": "^8.1",
        "drupal/admin_toolbar": "^8.1",
        "drupal/core": "~8.0",
        "drupal/csv_serialization": "8.1.0-alpha1",
        "drupal/metatag": "^8.1",
        "drupal/paragraphs": "^8.1@RC",
        "drupal/pathauto": "^8.1",
        "drupal/coffee": "^8.1"
    },
    "require-dev": {
        "behat/behat": "3.1.*@dev",
        "drupal/console": "1.0.0-alpha2",
        "drupal/devel": "8.1.x-dev",
        "drupal/drupal-extension": "3.1.*@dev"

    },
    "config": {
        "bin-dir": "bin",
        "process-timeout": 1200
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "installer-paths": {
            "www/core": ["type:drupal-core"],
            "www/modules/contrib/{$name}": ["type:drupal-module"],
            "www/profiles/contrib/{$name}": ["type:drupal-profile"],
            "www/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        }
    }

}
